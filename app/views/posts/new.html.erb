<%= form_for @post, url: posts_path do |f| %>
  <%= f.text_field :title %><br />
  <%= f.text_area :body, size: "60x12" %><br />
  <%= f.submit "Create" %>
<% end %>
<%= form_for @user, url: users_path, html: { multipart: true } do |form| %>
  <%= form.file_field :avatar %>
  <%= form.submit %>
<% end %>